<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_start_grading"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img"
    tools:context="com.kaustubh.rubrics.MainStartGrading">

    <TextView
        android:text="START GRADING"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView83"
        android:textColor="@android:color/background_light"
        android:textSize="30sp" />

    <Button
        android:text="VIEW STDENT GRADES"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/viewgrade"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="113dp"
        android:layout_alignRight="@+id/startgrade"
        android:layout_alignEnd="@+id/startgrade"
        android:layout_alignLeft="@+id/startgrade"
        android:layout_alignStart="@+id/startgrade" />

    <Button
        android:text="START GARDING STUDENTS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="109dp"
        android:id="@+id/startgrade"
        android:layout_below="@+id/textView83"
        android:layout_alignLeft="@+id/textView83"
        android:layout_alignStart="@+id/textView83"
        android:layout_alignRight="@+id/textView83"
        android:layout_alignEnd="@+id/textView83" />
</RelativeLayout>
