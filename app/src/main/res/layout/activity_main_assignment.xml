<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_assignment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img"
    tools:context="com.kaustubh.rubrics.MainAssignment">

    <Button
        android:text="VIEW ASSIGNMENT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/addac"
        android:layout_marginTop="102dp"
        android:id="@+id/viewac"
        android:layout_alignLeft="@+id/addac"
        android:layout_alignStart="@+id/addac"
        android:layout_alignRight="@+id/addac"
        android:layout_alignEnd="@+id/addac" />

    <Button
        android:text="ADD ASSIGNMENT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="126dp"
        android:id="@+id/addac"
        android:layout_below="@+id/textView81"
        android:layout_alignRight="@+id/textView81"
        android:layout_alignEnd="@+id/textView81"
        android:layout_alignLeft="@+id/textView81"
        android:layout_alignStart="@+id/textView81" />

    <TextView
        android:text="MANAGE ASSIGNMENT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView81"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
